not buy more than 10 items.\")\n                } else {\n                    this.counter++\n                }\n                this.$emit('counter', this.item, this.counter)\n            },\n            decrement:function() {\n                if (this.counter > 0) {\n                    this.counter--\n                }\n                this.$emit('counter', this.item, this.counter)\n            }\n        }\n    }\n</script>\n\n<style scoped>\n* {\n    font-family: Arial, Helvetica, sans-serif;\n    font-size: 30px;\n}\nbutton {\n    background-color: #80ffd4;\n    border-radius: 4px;\n    border: none;\n    height: 50px;\n    width: 50px;\n    text-align:center;\n    margin: 5px;\n}\n</style>"],"sourceRoot":"src/components"}]}