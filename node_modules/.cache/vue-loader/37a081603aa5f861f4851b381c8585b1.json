(var i = 0; i < this.selectedItems.length; i++) {\n                    var curr = this.selectedItems[i];\n                    this.subtotal += curr[1] * curr[2];\n                }\n            },\n            sendOrder: function() {\n                for (var i = 0; i < this.selectedItems.length; i++) {\n                    var curr = this.selectedItems[i]\n                    database.collection('orders').add({\n                        name: curr[0],\n                        price: curr[1],\n                        quantity: curr[2]\n                    })\n                }\n            }\n        },\n        computed : {\n            grandTotal: function() {\n                return (this.subtotal * 1.07).toFixed(2);\n            }\n        }\n    }\n</script>\n\n<style>\n#bill {\n    background-color: #80ffd4;\n    border-radius: 4px;\n    border: 1px solid #000000;\n    text-align:center;\n    margin: 5px;\n    font-size: 15px;\n    padding: 5px;\n}\n</style>"]}]}